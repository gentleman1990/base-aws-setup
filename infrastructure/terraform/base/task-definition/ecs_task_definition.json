[
  {
    "name": "simple-app",
    "image": "docker.io/yeasy/simple-web",
    "cpu": 10,
    "memory": 512,
    "essential": true,
    "privileged": true,
    "portMappings": [
      { "containerPort": 80, "hostPort": 80 }
    ],
    "logConfiguration": {
       "logDriver": "json-file",
       "options": {
           "max-size": "50m",
           "max-file": "5"
       }
    }
  }
]